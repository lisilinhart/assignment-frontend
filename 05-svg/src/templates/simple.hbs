<h3>Simple</h3>

<div class="row">
  <div class="col-sm-6">
    <h4>A rectangle</h4>
    <svg width="200px" height="100px" viewBox="0 0 204 104">
      <rect x="1" y="1" fill="#F7931E"  width="200" height="100" stroke="#C1272D" stroke-width="2"/>
    </svg>
  </div>
  <div class="col-sm-6">
    <h4>A circle</h4>
    <svg viewBox="0 0 500 100">
      <circle r="48" cx="50" cy="50" fill="#C1272D" stroke="#F7931E" stroke-width="2"/>
    </svg>
  </div>
</div>

<div class="row">
  <div class="col-sm-6">
    <h4>A group</h4>
    <svg viewBox="0 0 100 50">
      <g fill="#F7931E" stroke="#C1272D">
        <rect x="20" y="20" width="50" height="20" stroke-width="1" />
        <circle r="10" cx="20" cy="20" fill="#C1272D" stroke-width="1" />
      </g>
    </svg>
  </div>
  <div class="col-sm-6">
    <h4>A clipped text</h4>
    <svg viewBox="0 0 100 50">
      <defs>
        <clipPath id="clipPath">
          <text x="10" y="22" style="font-size: 5px; ">May the force be with you</text>
        </clipPath>
      </defs>
      <g fill="#F7931E" stroke="#C1272D " style="clip-path: url(#clipPath); ">
        <rect x="20" y="20" width="50" height="20" stroke-width="1" />
        <circle r="10" cx="20" cy="20" fill="#C1272D" stroke-width="1" />
      </g>
    </svg>
  </div>
</div>

